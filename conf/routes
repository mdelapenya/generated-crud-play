# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET / controllers.Application.index()

# Show all models
GET /showModels controllers.Application.models()

# Map static resources from the /public folder to the /assets URL path
GET /assets/*file controllers.Assets.at(path="/public", file)

# Upload SQL file
POST /upload controllers.Application.uploadSQL

##
## Model routes
##

# Account

# View a account
GET /accounts/:id controllers.AccountApplication.get(id: Long)

# Go to the create account view
GET /account/add controllers.AccountApplication.addAccount

# Display all accounts
GET /accounts controllers.AccountApplication.all

# Save a account
POST /account/submit controllers.AccountApplication.submit()

