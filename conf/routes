GET /webdavpropss/:id controllers.WebdavpropsApplication.get(id: Long)

# Go to the create webdavprops view
GET /webdavprops/add controllers.WebdavpropsApplication.addWebdavprops

# Display all webdavpropss
GET /webdavpropss controllers.WebdavpropsApplication.all

# Save a webdavprops
POST /webdavprops/submit controllers.WebdavpropsApplication.submit()

# Website

# View a website
GET /websites/:id controllers.WebsiteApplication.get(id: Long)

# Go to the create website view
GET /website/add controllers.WebsiteApplication.addWebsite

# Display all websites
GET /websites controllers.WebsiteApplication.all

# Save a website
POST /website/submit controllers.WebsiteApplication.submit()

# Workflowdefinitionlink

# View a workflowdefinitionlink
GET /workflowdefinitionlinks/:id controllers.WorkflowdefinitionlinkApplication.get(id: Long)

# Go to the create workflowdefinitionlink view
GET /workflowdefinitionlink/add controllers.WorkflowdefinitionlinkApplication.addWorkflowdefinitionlink

# Display all workflowdefinitionlinks
GET /workflowdefinitionlinks controllers.WorkflowdefinitionlinkApplication.all

# Save a workflowdefinitionlink
POST /workflowdefinitionlink/submit controllers.WorkflowdefinitionlinkApplication.submit()

# Workflowinstancelink

